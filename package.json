  "private": false,
  "license": "MIT",
  "main": "lib/vue-multiselect.min.js",
  "jsnext:main": "lib/vue-multiselect.min.js",
  "jspm": {
    "main": "lib/vue-multiselect.min.js",
    "registry": "npm",
    "format": "esm"
  },
  "repository": {
    "type": "git",
